<view class="c-minheight wx-view_LUuvj9" data-c_e_id="wx_view_92c6b2a3">
    <map
    id="map"
    latitude="39.940690"
    longitude="116.403350"
    scale="6"
    controls="{{controls}}"
    bindcontroltap="controltap"
    markers="{{markers}}"
    bindmarkertap="markertap"
    polyline="{{polyline}}"
    bindregionchange="regionchange"
    show-location
    style="width: 100%; height: 400px;"
    >
        <cover-view bindtap="onFloatIconClick">
            <cover-image class="floaticon" src="/images/map_marker.png"></cover-image>
        </cover-view>
    </map>

</view>
